{
  "name": "next-js-mongo-passport",
  "version": "1.0.0",
  "description": "Application for managing functions, including messages, from various outside applications",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd client && node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*' server.js\" \"npm run client\"",
    "client": "cd client && npm run dev",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "postinstall": "cd client && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nick-ramsay/next-js-mongo-passport.git"
  },
  "author": "Nick Ramsay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nick-ramsay/next-js-mongo-passport/issues"
  },
  "homepage": "https://github.com/nick-ramsay/next-js-mongo-passport#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "concurrently": "^5.2.0",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.18.1",
    "googleapis": "^150.0.1",
    "if-env": "^1.0.4",
    "install": "^0.13.0",
    "js-sha256": "^0.9.0",
    "mongoose": "^8.16.3",
    "nodemailer": "^6.4.10",
    "nodemon": "^2.0.4",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0"
  }
}
